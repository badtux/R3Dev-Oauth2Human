{
    "name": "badtux/r3dev-oauth2human",
    "description": "Human OAuth 2.0 support for the PHP League's OAuth 2.0 Client",
    "license": "MIT",
    "authors": [
        {
            "name": "Indika Gunasekara",
            "email": "indigun89@gmail.com"
        }
    ],
    "keywords": [
        "oauth",
        "oauth2",
        "client",
        "authorization",
        "authentication",
        "human"
    ],
    "require": {
        "php": ">=5.5.0",
        "league/oauth2-client": "~1.0@dev"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"

    },
    "autoload": {
        "psr-4": {
